<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.casestudyclient.ClientController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Button fx:id="button_send" layoutX="404" layoutY="338" text="Send" />
        <TextField fx:id="text_message" layoutX="30" layoutY="338" prefWidth="361" prefHeight="26" />
        <ScrollPane fx:id="sp_main" layoutY="70" layoutX="30" prefHeight="256" prefWidth="418" fitToWidth="true">
            <content>
                <VBox fx:id="vbox_message" prefHeight="248" prefWidth="412" />
            </content>
        </ScrollPane>
        <Label layoutX="200" layoutY="26" text="Client">
            <font>
                <Font name="Time New Roman" size="23"/>
            </font>
        </Label>
    </children>
</AnchorPane>
